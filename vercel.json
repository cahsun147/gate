{
  "version": 2,
  "builds": [
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "api/index.go"
    },
    {
      "src": "/test/dex",
      "dest": "api/index.go"
    },
    {
      "src": "/api",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/ai/(.*)", 
      "dest": "api/index.go"
    },
    {
      "src": "/api/dex",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/dex(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/tokens/holder/(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/tokens/traders/(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/wallet/holdings/(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/wallet/flow/(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/wallet/activity/(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/wallet/statistics/(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/api/v1/tokens/trends/(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/api/(.*)",
      "dest": "package.json"
    },
    {
      "src": "/(.*)",
      "dest": "package.json"
    }
  ]
}